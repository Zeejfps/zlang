struct Allocator = std.mem.Allocator;

struct HeapAllocatorData {
    buffer: u8[]
}

func create() -> Allocator {
    const data = malloc<HeapAllocatorData>()
    return {
        data: data,
        alloc: &alloc,
        free: &free,
    }
}

func alloc<T>(data: *) -> T* {
    const heap_allocator_data = (HeapAllocatorData*)data
}

func free<T>(data: *){
    const heap_allocator_data = (HeapAllocatorData*)data
}